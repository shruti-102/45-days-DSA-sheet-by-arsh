class Solution {
public:
    int height(TreeNode* root)
    {
        if(root==NULL) return 0;
        return max(height(root->left),height(root->right))+1;
    }
    int diameterOfBinaryTree(TreeNode* root) {
        if(root==NULL) return 0;
        int h1=height(root->left);
        int h2=height(root->right);
        cout<<h1<<" "<<h2<<endl;
        return h1+h2;
    }
};
